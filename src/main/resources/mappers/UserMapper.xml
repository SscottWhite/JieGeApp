<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ncstudy.mapper.UserMapper">

    <select id="getUser" resultType="com.ncstudy.pojo.User">
       	select id, user_name as name, user_age as age, user_permission as permission, user_gender as gender from user;
    </select>

</mapper>